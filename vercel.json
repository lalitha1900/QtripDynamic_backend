{
  "version":2,
  "builds":[
    {
      "src":"index.js",
      "use":"@now/node"
    }
  ],
  "rewrites":[{
    "source":"/(.*)",
    "destination":"src/index.js"
   
  }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    } 
]
}